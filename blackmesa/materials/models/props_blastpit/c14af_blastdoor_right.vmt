"VertexLitGeneric"
{
	"$baseTexture" "models/props_blastpit/c14af_blastdoor_right"
	"$bumpmap" "models/props_blastpit/c14af_blastdoor_right_Normal"
	"$envmap" "env_cubemap"
   	"$envmaptint" "[.01 .01 .01]"
	"$normalalphaenvmapmask" 1	
	"%keywords" "bms"

$phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "80"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
}
