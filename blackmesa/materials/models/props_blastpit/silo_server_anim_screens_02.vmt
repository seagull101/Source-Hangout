"VertexLitGeneric"
{
	"$baseTexture" "models/props_blastpit/silo_server_anim_screens_02"
	"$angle" "0.0"
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
			"rate" "-90"
			"initialValue" "0.0"
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}
