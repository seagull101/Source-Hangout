"VertexLitGeneric"
{
	"$baseTexture" 	"models/props_industrial/Barrel_Drainer"
	"$bumpmap"	"models/props_industrial/Barrel_Drainer_Normal"
	"$envmap" "env_cubemap"
   	"$envmaptint" "[.03 .03 .03]"
	"$normalalphaenvmapmask" 1	
	"%keywords" "bms"

$phong 1
        $phongexponent 25
        $phongboost 1
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "45"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".15"	               // Boost for ambient cube component of rim lighting
}
