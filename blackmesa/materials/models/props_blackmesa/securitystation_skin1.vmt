"VertexLitGeneric"
{
	"$basetexture" "models/props_blackmesa/securitystation_skin1"
	"$bumpmap" "models/props_blackmesa/securitystation_normal_skin1"
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" "1"
	"$envmapcontrast" "0.8"
	"$envmapsaturation" "0.6"
"$envmaptint" "[.2 .2 .2]"
$phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "80"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
}
