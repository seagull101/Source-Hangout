"VertexLitGeneric"
{
	"$basetexture" "models/props_blackmesa/console01_tan_on"
	"$bumpmap" "models/props_blackmesa/console01_normal"
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" 1
	"$envmapcontrast" 1
	"$envmapsaturation" 1
	"$envmaptint" "[ .7 .7 .2 ]"
	"$selfillum" "1"

$phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "80"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
}
