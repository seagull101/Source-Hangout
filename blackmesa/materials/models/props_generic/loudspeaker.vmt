"VertexLitGeneric"
{
	"$baseTexture" "models\props_generic/loudspeaker"
	"$basealphaenvmapmask" 1
	"$surfaceprop" "plastic"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.3 .3 .3]"
  $phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
