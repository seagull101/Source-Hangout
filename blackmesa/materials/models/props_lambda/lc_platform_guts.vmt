"VertexLitGeneric"
{
	"$baseTexture" 	"models/props_lambda/lc_platform_guts"
	"$bumpmap"	"models/props_lambda/lc_platform_guts_normal"
	"$envmap" "env_cubemap"
	"$normalmapalphaenvmapmask" 1	
   	"$envmaptint" "[.6 .5 .3]"
	"$envmapsaturation" 	"1"

        "$detail" "detail\bm_metal_detail_01"
	"$detailscale" "5"
	"$detailblendfactor" .8
