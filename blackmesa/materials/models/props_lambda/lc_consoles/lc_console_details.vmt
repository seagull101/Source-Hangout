"VertexLitGeneric"
{
	"$basetexture" "models\props_lambda\lc_consoles\lc_console_details"
	"$bumpmap" "models\props_lambda\lc_consoles\lc_console_details_normal"
//	"$envmap" "env_cubemap"
//   	"$normalmapalphaenvmapmask" "1"
//	"$envmaptint" "[ .5 .5 .5 ]"
	"$selfillum"	"1"
	"$model" "1"
"$phong" "1"
"$phongexponent" "25"
"$phongboost" "2"
"$phongfresnelranges"	"[0.4 0.6 1.6]"
"$phongalbedotint" "1"



// Rim lighting parameters
"$rimlight" "1"
"$rimlightexponent" "15"
"$rimlightboost" "0.2"
"Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" "5"
      }
   }
}

