"vertexlitgeneric" 
{
	"$baseTexture" "models/water/waterplane1"
	"$translucent" "1"
	"$vertexalpha" 1
	"$nocull" "1"
	"%keywords" "bms"
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$baseTexturetransform"
			"texturescrollrate" .6
			"texturescrollangle" 270
		}

		
	}
}
}

