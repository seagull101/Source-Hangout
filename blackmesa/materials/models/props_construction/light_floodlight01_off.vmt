"VertexLitGeneric"
{
	"$basetexture" "models/props_construction/light_floodlight01_off"
	"$envmap" "env_cubemap"
	"$envmapmask" "models/props_construction/light_floodlight01_ref"
	"$envmapsaturation" "0.4"
	"$envmaptint" "[ 0.6 0.6 0.6 ]"

$phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "80"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
}
