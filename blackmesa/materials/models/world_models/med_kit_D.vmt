"VertexLitGeneric"
{
	"$baseTexture" "models\world_models/med_kit_D"
	"$bumpmap" "models\world_models/med_kit_N"
	"$envmap" "env_cubemap"
	"$envmaptint" "[1 1 1]"
	"$basealphaenvmapmask" "0"

	"$selfillum" "1"
	"$selfillummask"  "models\world_models/med_kit_SI"
	"$selfillumtint"	"[0.9 1.2 1.2]"

//	"$selfIllumFresnel" "1"
//	"$selfIllumFresnelMinMaxExp" "[0.1 1 2]"


	"$nocull" 1
	"$model" "1"
	"$halflambert" "1"

// -- From here down is new stuff which will only be applied if $phong is set to 1 --
        $phong 1
	$phongexponent "8"

        $phongboost "2.4"
        $phongfresnelranges "[0.5 2 4]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "5"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".5"	               // Boost for ambient cube component of rim lighting
}
