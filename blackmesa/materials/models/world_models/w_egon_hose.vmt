"VertexLitGeneric"
{
	"$baseTexture" "models\world_models/w_egon_hose"
"$bumpmap" "models\world_models\w_egon_hose_nm"
    	"$normalmapalphaenvmapmask" "1"
$phong 1
        $phongexponent 80
        $phongboost 1
        $halflambert 0
