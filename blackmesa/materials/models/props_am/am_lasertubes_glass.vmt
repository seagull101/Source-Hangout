//"EDITOR_SHADER"
"VertexLitGeneric"
{
"$SHADERNAME" "model_standard"

"$basetexture" "models\props_am\am_lasertubes_glass"
"$bumpmap" "models\props_am\am_lasertubes_glass_norm"

"$translucent"	"1"

//metal
"$normalmapalphaenvmapmask" "1"
"$envmap" "env_cubemap"
"$envmaptint" "[.6 .6 .6]"
//"$envmapfalloff" "1"

// Phong Schtuff
"$phong" "1"
"$phongexponent" "100"
"$PhongWarpTexture"	"models\props_am\am_ams_phongwarp"
"$phongboost" "6"
"$phongfresnelranges"	"[0.4 0.6 1.6]"
"$phongalbedotint" "1"

"$lightwarptexture"	"models\props_am\am_ams_lightwarp"

// Rim lighting parameters
"$rimlight" "1"
"$rimlightexponent" "25"
"$rimlightboost" "0.5"

$detail "detail/bm_metal_detail_01.vtf"
$detailscale 8
$detailblendfactor .6
$detailblendmode 0
}