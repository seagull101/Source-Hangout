//"EDITOR_SHADER"
"VertexLitGeneric"
{
"$SHADERNAME" "model_standard"

"$basetexture" "models\props_am\am_ams_tracks"
//"$basetexture" "models\props_am\am_ams_testgrey"
"$bumpmap" "models\props_am\am_ams_tracks_norm"

//metal
"$normalmapalphaenvmapmask" "1"
"$envmap" "env_cubemap"
"$envmaptint" "[1 .6 .4]"
//"$envmapfalloff" "1"

// Phong Schtuff
"$phong" "1"
"$phongexponent" "50"
"$PhongWarpTexture"	"models\props_am\am_ams_phongwarp"
"$phongboost" "7"
"$phongfresnelranges"	"[0.4 0.6 1.6]"
"$phongalbedotint" "1"

"$lightwarptexture"	"models\props_am\am_ams_lightwarp"

// Rim lighting parameters
"$rimlight" "1"
"$rimlightexponent" "25"
"$rimlightboost" "0.5"

$detail "detail/bm_abusedmetal_detail_01.vtf"
$detailscale 4
$detailblendfactor .7
$detailblendmode 0
}