//"EDITOR_SHADER"
"VertexLitGeneric"
{
"$SHADERNAME" "model_standard"

"$basetexture" "models\props_am\am_ams_basearm"
//"$basetexture" "models\props_am\am_ams_testgrey"
"$bumpmap" "models\props_am\am_ams_basearm_norm"

//metal
"$normalmapalphaenvmapmask" "1"
"$envmap" "env_cubemap"
"$envmaptint" "[.2 .2 .1]"
//"$envmapfalloff" "1"

// Phong Schtuff
"$phong" "1"
//"$phongexponent" "25"
"$phongexponenttexture" "models\props_am\am_ams_basearm_exponent"
"$PhongWarpTexture"	"models\props_am\am_ams_phongwarp"
"$phongboost" "6"
"$phongfresnelranges"	"[0.4 0.6 1.6]"
"$phongalbedotint" "1"

"$lightwarptexture"	"models\props_am\am_ams_lightwarp"

// Rim lighting parameters
"$rimlight" "1"
"$rimlightexponent" "25"
"$rimlightboost" "0.5"

$detail "detail/bm_abusedmetal_detail_01.vtf"
$detailscale 16
$detailblendfactor .7
$detailblendmode 0
}