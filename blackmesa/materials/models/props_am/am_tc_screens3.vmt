"UnLitGeneric"
{
	"$baseTexture" "models/props_am/am_tc_screens3"
	"$model" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[ .5 .5 .5 ]"
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" 	"$baseTextureTransform"
			"texturescrollrate" 	.07
		}


		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "0.5"
		}

	}
}
