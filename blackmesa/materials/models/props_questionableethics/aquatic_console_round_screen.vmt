"UnlitTwoTexture"
{
	"$basetexture" "Models/props_questionableethics/aquatic_console_round_screen_radar"
	"$texture2" "Models/props_questionableethics/aquatic_console_round_screen_sweep"
	"$model" "1"
	"$decal" "1"
	"$additive" "1"


        "$angle" 0.0
        "$translate" "[0.0 0.0]"
        "$center" "[0.5 0.5]"

        "Proxies"
        {
            "LinearRamp"
            {
                "rate" 90
                "initialValue" 0.0
                "resultVar" "$angle"
            }

            "TextureTransform"
            {
                "translateVar" "$translate"
                "rotateVar" "$angle"
                "centerVar" "$center"
		"resultVar" "$texture2transform"
            }
        }
    }
