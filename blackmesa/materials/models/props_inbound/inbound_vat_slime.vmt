"VertexlitGeneric"
{
	"$baseTexture" "models/props_inbound/inbound_vat_slime"
"$bumpmap" "models/props_inbound/inbound_vat_slime_normal"
	"$normalmapalphaenvmapmask" "1"
	"$envmap" "env_cubemap"
"$envmaptint" "[.2 .2 1]"
	"$selfillum"	"1"
	"$model" "1"
$phong 1
        $phongexponent 7
        $phongboost 10
        $phongtint "[.1 .1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "80"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" 	"$baseTextureTransform"
			"texturescrollrate" 	.07
			"textureScrollAngle"	"-90"
		}
	}
}
