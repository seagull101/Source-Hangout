"UnlitGeneric"
{
	"$basetexture"	"vgui/screens/mortar/icon_mortar_ping"
	"$translucent" 1
	"$additive" 1
	"$alpha" 1
	"$vertexalpha" "1"
	"$ignorez"     "1"
	"$no_fullbright" "1"	
	"$vertextcolor" "1"
	
	"$texoffset" "[0 0]"
	"$texscale" "1"

	"Proxies"
	{
		"Sine"
		{
			"resultVar" "$texscale"
			"sineperiod" "1"
			"sinemin" "0.8"
			"sinemax" "1.2"
		}
		
		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
	}
}